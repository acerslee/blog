<%- include("partials/header"); -%>

<h1>Compose</h1>
<form class = "item" action = "/compose" method = "post">
    <div class = "form-group">
        <h3>Title</h3>
        <input class = "form-control" type = "text" name="composeTitle">
    </div>
    
    <div class = "form-group">
        <h3>Post</h3>
        <textarea class = "form-control" rows="4" type = "text" name="composePost"></textarea>
    </div>

    <button class="btn btn-primary" type="Submit">Publish</button>
</form>

